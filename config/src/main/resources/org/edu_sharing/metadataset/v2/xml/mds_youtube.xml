<metadataset id="default" label="default" hidden="false">
		<name>Youtube Metadataset</name>
		<inherit>mds</inherit>
		<i18n>mds</i18n>
		<widgets>
		</widgets>
		<sorts>
			<sort>
				<id>search</id>
				<default>
					<sortBy>score</sortBy>
					<sortAscending>false</sortAscending>
				</default>
				<!-- no columns because we currently don't support sorting for this repository -->
			</sort>
		</sorts>
		<lists>
			<list>
				<id>search</id>
				<columns>
					<column>cm:title</column>
					<column>cm:creator</column>
					<column>cm:modified</column>
					<column>ccm:replicationsource</column>				
				</columns>
			</list>
		</lists>
		<templates>
			<template>
				<id>search</id>
				<html><![CDATA[
              	<div>{{no_search_filters}}</div>
				]]></html>
			</template>
			<template>
				<id>node_general_render</id>
				<caption>dialog_upload_tab_basic</caption>
				<html><![CDATA[
              <cclom:title>
              <sys:node-uuid>
				]]></html>
			</template>
		</templates>
		<groups>
			<group>
				<id>ngsearch</id>
				<views>
					<view>search</view>
				</views>
			</group>
		</groups>
</metadataset>